{
  "version": "1.0.0",
  "description": "MealPlanana is a MERN Stack web app that allows users to share recipes and plan out their meals on a calendar.   It is currently being hosted [here](https://mealplanana.herokuapp.com/).",
  "main": "./backend/src/index.js",
  "module": "./backend/src/server.js",
  "scripts": {
    "start": "cd backend && npm install && node src/index.js",
    "dev": "nodemon backend/src/index.js",
    "front": "cd frontend && npm start",
    "stack": "concurrently --kill-others-on-fail \"npm run dev\" \"npm run front\"",
    "heroku-postbuild": "cd frontend && npm install --only=dev && npm install && npm run build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.862.0",
    "bcryptjs": "^2.4.3",
    "concurrently": "^6.0.0",
    "connect-mongo": "^3.2.0",
    "crypto-js": "^4.0.0",
    "dotenv": "^8.2.0",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "joi": "^17.4.0",
    "mongoose": "^5.11.16",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "eslint": "^7.19.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-only-warn": "^1.0.2",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "nodemon": "^2.0.7"
  },
  "name": "mealplanana",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ivanma9/MealPlanana.git"
  },
  "bugs": {
